  "author": "Jun Kato <jun.kato@aist.go.jp> (https://junkato.jp)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TextAliveJp/textalive-app-params/issues"
  },
  "homepage": "https://github.com/TextAliveJp/textalive-app-params#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-ui-react": "^2.1.5",
    "styled-components": "^6.1.8",
    "textalive-app-api": "^0.3.0",
    "textalive-react-api": "^0.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
