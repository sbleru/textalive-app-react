    "del-cli": "^6.0.0",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "lefthook": "^1.11.13",
    "prettier": "^3.5.3",
    "typescript": "^5.4.0",
    "vite": "^5.1.0"
  },
  "packageManager": "npm@10.8.2"
